<%- include('../../partials/head'); %>
<%- include('../../partials/header'); %>

<main>
  <h1>RESULTATEN BUCKETLIST test</h1>
  <p>Welcome to Backend A2 Assignment</p>

  <div class="countryResults">
    <div>
      <!-- loop trough the schema object -->

      <% if (countryView.length > 0) { %>
      <% countryView.forEach(view => { %>

      <a class='singleCountry' href="/bucketlistOverview/<%= view._id%>">
        <p class='oneCountry'> <%= view.countryOne %></p>
        <p class='oneCountry'> <%= view.countryTwo %></p>
        <p class='oneCountry'> <%= view.countryThree %></p>
      </a>

      <% }) %>
      <% } else {%>
      <p>nog geen interesses toegevoegd</p>
      <% }%>
    </div>
  </div>

</main>

<%- include('../../partials/footer'); %>